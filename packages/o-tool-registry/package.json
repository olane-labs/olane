{
  "name": "@olane/o-tool-registry",
  "version": "0.7.5",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "aegir test",
    "test:node": "aegir test -t node",
    "test:browser": "aegir test -t browser",
    "deep:clean": "rm -rf node_modules && rm package-lock.json",
    "dev": "npm run build --prefix ../o-core && npm run build --prefix ../o-tool && npm run build --prefix ../o-tools-common && DEBUG=o-protocol:* npx tsx src/index.ts",
    "build": "tsc",
    "start:prod": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "update:peers": "npm install @olane/o-config@latest @olane/o-core@latest @olane/o-intelligence@latest @olane/o-mcp@latest @olane/o-protocol@latest @olane/o-tool@latest @olane/o-tools-common@latest @olane/o-lane@latest --save-peer",
    "lint": "aegir lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olane-labs/olane.git"
  },
  "author": "oLane Inc.",
  "license": "(MIT OR Apache-2.0)",
  "description": "oLane server tool registry",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@tsconfig/node20": "^20.1.6",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "aegir": "^47.0.21",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.6",
    "eslint-plugin-prettier": "^5.5.0",
    "globals": "^16.2.0",
    "jest": "^30.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
    "@olane/o-config": "^0.7.4",
    "@olane/o-core": "^0.7.4",
    "@olane/o-intelligence": "^0.7.4",
    "@olane/o-lane": "^0.7.4",
    "@olane/o-mcp": "^0.7.4",
    "@olane/o-protocol": "^0.7.4",
    "@olane/o-tool": "^0.7.4",
    "@olane/o-tools-common": "^0.7.4"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.5.2",
    "@langchain/community": "^0.3.47",
    "@langchain/core": "^0.3.61",
    "debug": "^4.4.1",
    "dotenv": "^16.5.0",
    "langchain": "^0.3.36"
  }
}
