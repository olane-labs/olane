{
  "name": "@olane/o-network",
  "version": "0.6.7",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "aegir test",
    "test:node": "aegir test -t node",
    "test:browser": "aegir test -t browser",
    "test-0": "npm run build:all && DEBUG=o-protocol:* npx tsx src/tests/test-0.ts",
    "deep:clean": "rm -rf node_modules && rm package-lock.json",
    "deep:clean:all": "npm run deep:clean && npm run deep:clean --prefix ../o-tool && npm run deep:clean --prefix ../o-tools-common && npm run deep:clean --prefix ../o-tool-registry && npm run deep:clean --prefix ../..",
    "deep:install": "npm install && npm install --prefix ../o-tool && npm install --prefix ../o-tools-common && npm install --prefix ../o-tool-registry && npm install --prefix ../..",
    "deep:restart": "npm run deep:clean:all && npm run deep:install && npm run build:all",
    "build:all": "npm run build --prefix ../.. && npm run build --prefix ../o-tool && npm run build --prefix ../o-tools-common && npm run build --prefix ../o-tool-registry && npm run build --prefix ../o-mcp",
    "build": "tsc",
    "start:prod": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "update:lib": "npm install @olane/o-core@latest",
    "lint": "aegir lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olane-labs/olane.git"
  },
  "author": "oLane Inc.",
  "license": "ISC",
  "description": "oLane Core",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@tsconfig/node20": "^20.1.6",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/touch": "^3.1.5",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "aegir": "^47.0.21",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.6",
    "eslint-plugin-prettier": "^5.5.0",
    "globals": "^16.2.0",
    "jest": "^30.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
    "@olane/o-config": "^0.6.7",
    "@olane/o-core": "^0.6.7",
    "@olane/o-intelligence": "^0.6.7",
    "@olane/o-leader": "^0.6.7",
    "@olane/o-protocol": "^0.6.7",
    "@olane/o-storage": "^0.6.7",
    "@olane/o-tool": "^0.6.7",
    "@olane/o-tool-registry": "^0.6.7",
    "@olane/o-tools-common": "^0.6.7"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "debug": "^4.4.1",
    "dotenv": "^16.5.0",
    "fs-extra": "^11.3.0",
    "touch": "^3.1.1"
  }
}
