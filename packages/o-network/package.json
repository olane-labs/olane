{
  "name": "@olane/o-network",
  "version": "0.2.2",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test-0": "npm run build:all && DEBUG=o-protocol:* npx tsx src/tests/test-0.ts",
    "test-1": "npm run build:all && DEBUG=o-protocol:* npx tsx src/tests/test-1.ts",
    "test-2": "npm run build:all && DEBUG=o-protocol:* npx tsx src/tests/test-2.ts",
    "test-old": "npm run build:all && DEBUG=o-protocol:* npx tsx src/tests/old.ts",
    "build:all": "npm run build --prefix ../.. && npm run build --prefix ../o-tool && npm run build --prefix ../o-tools-common && npm run build --prefix ../o-tool-registry",
    "test": "npm run build:all && DEBUG=o-protocol:* npx tsx src/tests/test-1.ts",
    "net": "npm run build:all && DEBUG=o-protocol:* npx tsx src/tests/network.ts",
    "cli": "npm run build:all && DEBUG=o-protocol:* npx tsx src/index.ts",
    "build": "tsc",
    "start:prod": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "update:lib": "npm install @olane/o-core@latest",
    "lint": "eslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olane-labs/olane.git"
  },
  "author": "oLane Inc.",
  "license": "ISC",
  "description": "oLane Core",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@olane/o-config": "file:../o-config",
    "@olane/o-core": "file:../..",
    "@olane/o-protocol": "file:../o-protocol",
    "@olane/o-tool": "file:../o-tool",
    "@olane/o-tool-registry": "file:../o-tool-registry",
    "@olane/o-tools-common": "file:../o-tools-common",
    "@tsconfig/node20": "^20.1.6",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/touch": "^3.1.5",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.6",
    "eslint-plugin-prettier": "^5.5.0",
    "globals": "^16.2.0",
    "jest": "^30.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
    "@olane/o-config": "^0.2.2",
    "@olane/o-core": "^0.2.2",
    "@olane/o-protocol": "^0.2.2",
    "@olane/o-tool": "^0.2.2",
    "@olane/o-tool-registry": "^0.2.2",
    "@olane/o-tools-common": "^0.2.2"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "debug": "^4.4.1",
    "dotenv": "^16.5.0",
    "fs-extra": "^11.3.0",
    "touch": "^3.1.1"
  }
}
