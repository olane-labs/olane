{
  "name": "@olane/o-monitor",
  "version": "0.7.10",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "aegir test",
    "test:node": "aegir test -t node",
    "test:browser": "aegir test -t browser",
    "dev": "DEBUG=o-protocol:* npx tsx src/tests/index.ts",
    "build": "tsc",
    "deep:clean": "rm -rf node_modules && rm package-lock.json",
    "start:prod": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "update:peers": "npm install @olane/o-config@latest @olane/o-core@latest @olane/o-intelligence@latest @olane/o-leader@latest @olane/o-protocol@latest @olane/o-tool@latest @olane/o-lane@latest @olane/o-node@latest @olane/o-storage@latest --save-peer",
    "update:lib": "npm install @olane/o-core@latest",
    "lint": "eslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olane-labs/olane.git"
  },
  "author": "oLane Inc.",
  "license": "(MIT OR Apache-2.0)",
  "description": "oLane Core",
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "^1.18.1",
    "@olane/o-config": "^0.7.9",
    "@olane/o-core": "^0.7.9",
    "@olane/o-intelligence": "^0.7.9",
    "@olane/o-lane": "^0.7.9",
    "@olane/o-leader": "^0.7.9",
    "@olane/o-node": "^0.7.9",
    "@olane/o-protocol": "^0.7.9",
    "@olane/o-storage": "^0.7.9",
    "@olane/o-tool": "^0.7.9"
  },
  "dependencies": {
    "@libp2p/prometheus-metrics": "^4.3.29",
    "debug": "^4.4.1",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "open": "^10.0.0",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@tsconfig/node20": "^20.1.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "aegir": "^47.0.21",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.6",
    "eslint-plugin-prettier": "^5.5.0",
    "globals": "^16.2.0",
    "jest": "^30.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "5.4.5"
  }
}
